
public class Strings {

	public static void main(String[] args) {
		// String Literal - String Constant Pool **PREFERRED**
		String str1 = "Hello";
		
		String str3 = "Hello";
		
		// String Object - Heap
		String str2 = new String("Welcome");
		
		String str4 = new String("Welcome");
		
		// Strings
		str1 = "Goodbye"; //duplicate removes/replaces the previous string
		
		String stradd = str1 + str2;
	}

}
